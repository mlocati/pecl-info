    </maintainer>
  </maintainers>
  <release>
    <version>0.1.0</version>
    <date>2004-04-01</date>
    <license>PHP</license>
    <state>devel</state>
    <notes>Initial release, supporting fann-1.1.0</notes>
    <filelist>
      <file role="src" name="config.m4"/>
      <file role="src" name="fann.c"/>

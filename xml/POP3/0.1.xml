<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>pop3</name>
  <summary>POP3 Client Library</summary>
  <description>The POP3 extension makes it possible for a PHP 
 script to connect to and interact with a POP3 mail server.
 Based on libspopc (http://brouits.free.fr/libspopc/), it is
 built for performance and ease of use.</description>
  <maintainers>
    <maintainer>
      <user>marcot</user>
      <name>Marco Tabini</name>
      <email>marcot@tabini.ca</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>iliaa</user>
      <name>Ilia Alshanetsky</name>
      <email>ilia@php.net</email>
      <role>developer</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1</version>
    <date>2004-01-10</date>
    <license>PHP</license>
    <state>alpha</state>
    <notes>Initial release</notes>
    <deps>
      <dep type="php" rel="ge" version="4"/>
    </deps>
    <configureoptions>
      <configureoption name="with-pop3" default="autodetect" prompt="POP3 libspopc library installation dir?"/>
    </configureoptions>
    <filelist>
      <file role="src" md5sum="ab29b439e1821a29f7b81754e66c7889" name="config.m4"/>
      <file role="src" md5sum="fadf8282a5aecaa6e77610d6700d1aab" name="config.w32"/>
      <file role="src" md5sum="faf99983ef7f70f285f9d61576b668d4" name="pop3.c"/>
      <file role="src" md5sum="6f91045fc3055a7650e2f5b7cb870235" name="php_pop3.h"/>
      <file role="doc" md5sum="d94947803c27298ebdbb3605413c73bc" name="CREDITS"/>
      <file role="doc" md5sum="accf2fb1ef48ecb4d9dee7acb8c3909a" name="EXPERIMENTAL"/>
      <file role="doc" md5sum="af10e386cb7fbb07c296de60446cd109" name="README"/>
      <file role="doc" md5sum="3cfcc2b7caa7ab74d37a7c71b78624ac" name="examples/pop3.php"/>
      <file role="doc" md5sum="b1143728f480a00af6d0a43486f57d8e" name="examples/pop3-oop.php"/>
    </filelist>
  </release>
</package>

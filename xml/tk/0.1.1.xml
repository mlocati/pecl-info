<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>tk</name>
  <summary>Extension for Tk (X-windows interface toolkit)</summary>
  <description>PHP/TK is an native extension for the PHP programming language that implements language bindings for TCL/TK. It provides an object-oriented interface and greatly simplifies writing client-side cross-platform GUI applications.</description>
  <maintainers>
    <maintainer>
      <user>ut0pia</user>
      <name>Mathieu Hurtevent</name>
      <email>mathieu.h@gmail.com</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1.1</version>
    <date>2004-11-14</date>
    <license>PHP</license>
    <state>beta</state>
    <notes>third release</notes>
    <filelist>
      <file role="src" name="config.m4"/>
      <file role="src" name="tk.c"/>
      <file role="src" name="php_tk.h"/>
      <file role="doc" name="CREDITS"/>
      <file role="doc" name="EXPERIMENTAL"/>
      <file role="test" name="tests/hello.phpt"/>
      <file role="test" name="tests/logo.phpt"/>
      <file role="test" name="bin/i586/tk.so"/>
    </filelist>
  </release>
</package>

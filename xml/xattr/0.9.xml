  <maintainers>
    <maintainer>
      <user>mg</user>
      <name>Marcin Gibula</name>
      <email>mg@iceni.pl</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.9</version>
    <date>2004-08-14</date>
    <license>PHP License</license>
    <state>beta</state>
    <notes>First public release. Requires libattr from Linux XFS project.</notes>
    <configureoptions>
      <configureoption name="with-xattr" default="autodetect" prompt="libattr library installation dir?"/>

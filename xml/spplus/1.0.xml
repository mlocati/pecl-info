  <maintainers>
    <maintainer>
      <user>nicos</user>
      <name>Chaillan Nicolas</name>
      <email>nicos@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0</version>
    <date>2002-11-08</date>
    <license>PHP License</license>
    <state>stable</state>
    <notes>Updated alpha release that includes a number of improvements over the existing alpha preview.  PEAR (PECL) support is now working, too.</notes>
    <configureoptions>
      <configureoption name="with-spplus" default="autodetect" prompt="SPPLUS installation directory?"/>

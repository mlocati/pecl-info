<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>xmlrpci</name>
  <summary>Improved XML-RPC</summary>
  <description>A re-write of the old ext/xmlrpc extension for PHP 5 implmenting object overloading
and built on top of the libxml2 library. It strives to be a completely backward
compatiable replacement to the ext/xmlrpc extension</description>
  <maintainers>
    <maintainer>
      <user>john</user>
      <name>John Coggeshall</name>
      <email>john@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0</version>
    <date>2005-02-15</date>
    <license>PHP</license>
    <state>beta</state>
    <notes>Initial release</notes>
    <deps>
      <dep type="php" rel="ge" version="5"/>
    </deps>
    <configureoptions>
      <configureoption name="enable-xmlrpci" default="autodetect" prompt="Enable Improved XML-RPC Support?"/>
    </configureoptions>
    <filelist>
      <file role="src" md5sum="1fde36c68263e779320245c42aa4c643" name="config.m4"/>
      <file role="src" md5sum="983d9566445eeaf36ddbe032d1fc87d1" name="config.w32"/>
      <file role="src" md5sum="2052d71bae289b600475a48d5b4f7c86" name="xmlrpci.c"/>
      <file role="src" md5sum="89909f2cb5f702c27df6c39ab9613a2a" name="php_xmlrpci.h"/>
      <file role="doc" md5sum="518a4d4ad3ffc9aa6f2f06acddcc6366" name="CREDITS"/>
      <file role="doc" md5sum="d41d8cd98f00b204e9800998ecf8427e" name="EXPERIMENTAL"/>
    </filelist>
  </release>
</package>

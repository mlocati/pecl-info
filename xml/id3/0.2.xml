<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>id3</name>
  <summary>Functions to read and write ID3 tags in MP3 files.</summary>
  <description>id3 enables to to retrieve and update information from ID3 tags in
MP3 files. It supports version 1.0, 1.1 and 2.2+ (only reading text- and url-frames at the moment).</description>
  <maintainers>
    <maintainer>
      <user>schst</user>
      <name>Stephan Schmidt</name>
      <email>schst@php-tools.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>luckec</user>
      <name>Carsten Lucke</name>
      <email>luckec@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.2</version>
    <date>2004-09-24</date>
    <license>PHP License</license>
    <state>alpha</state>
    <notes>added support to read id3v2 text- and url-frames</notes>
    <deps>
      <dep type="php" rel="ge" version="4.3.0" optional="no"/>
    </deps>
    <configureoptions>
      <configureoption name="enable-id3" default="autodetect" prompt="Enable id3 support?"/>
    </configureoptions>
    <filelist>
      <file role="doc" baseinstalldir="" name="examples/example_get.php"/>
      <file role="doc" baseinstalldir="" name="examples/example_get_genre_name.php"/>
      <file role="doc" baseinstalldir="" name="examples/example_id3v2_frames.php"/>
      <file role="doc" baseinstalldir="" name="examples/example_set.php"/>
      <file role="doc" baseinstalldir="" name="examples/example_version.php"/>
      <file role="src" baseinstalldir="" name="config.m4"/>
      <file role="src" baseinstalldir="" name="config.w32"/>
      <file role="data" baseinstalldir="" name="CREDITS"/>
      <file role="data" baseinstalldir="" name="EXPERIMENTAL"/>
      <file role="src" baseinstalldir="" name="id3.c"/>
      <file role="data" baseinstalldir="" name="id3.dsp"/>
      <file role="src" baseinstalldir="" name="php_id3.h"/>
      <file role="data" baseinstalldir="" name="TODO"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.1</version>
      <date>2004-09-24</date>
      <license>PHP License</license>
      <state>alpha</state>
      <notes>added support to read id3v2 text- and url-frames
</notes>
    </release>
    <release>
      <version>0.2</version>
      <date>2004-09-24</date>
      <license>PHP License</license>
      <state>alpha</state>
      <notes>added support to read id3v2 text- and url-frames
</notes>
    </release>
  </changelog>
</package>
